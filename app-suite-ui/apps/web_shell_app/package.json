{
  "name": "web_shell_app",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "web_shell_app",
    "sourceRoot": "apps/web_shell_app/src",
    "targets": {
      "build": {
        "executor": "@nx/webpack:webpack",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "compiler": "babel",
          "outputPath": "apps/web_shell_app/dist",
          "index": "apps/web_shell_app/src/index.html",
          "main": "apps/web_shell_app/src/main.ts",
          "tsConfig": "apps/web_shell_app/tsconfig.app.json",
          "styles": [
            "apps/web_shell_app/src/styles.css"
          ],
          "webpackConfig": "apps/web_shell_app/webpack.config.ts",
          "postcssConfig": "apps/web_shell_app/postcss.config.js"
        },
        "configurations": {
          "development": {
            "extractLicenses": false,
            "optimization": false,
            "sourceMap": true,
            "vendorChunk": true
          },
          "production": {
            "fileReplacements": [
              {
                "replace": "apps/web_shell_app/src/environments/environment.ts",
                "with": "apps/web_shell_app/src/environments/environment.prod.ts"
              }
            ],
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "namedChunks": false,
            "extractLicenses": true,
            "vendorChunk": false,
            "webpackConfig": "apps/web_shell_app/webpack.config.ts"
          }
        },
        "dependsOn": [
          "^build",
          "typecheck"
        ]
      },
      "serve": {
        "executor": "@nx/react:module-federation-dev-server",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "web_shell_app:build",
          "hmr": true,
          "port": 4200
        },
        "configurations": {
          "development": {
            "buildTarget": "web_shell_app:build:development"
          },
          "production": {
            "buildTarget": "web_shell_app:build:production",
            "hmr": false
          }
        },
        "dependsOn": [
          "typecheck"
        ]
      },
      "serve-static": {
        "executor": "@nx/react:module-federation-static-server",
        "defaultConfiguration": "production",
        "options": {
          "serveTarget": "web_shell_app:serve"
        },
        "configurations": {
          "development": {
            "serveTarget": "web_shell_app:serve:development"
          },
          "production": {
            "serveTarget": "web_shell_app:serve:production"
          }
        }
      }
    }
  },
  "devDependencies": {
    "web_app_1": "*"
  }
}
